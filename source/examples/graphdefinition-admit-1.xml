<?xml version="1.0" encoding="UTF-8"?>
<GraphDefinition xmlns="http://hl7.org/fhir">
<id value="admit-1"></id>
	<url value="http://hl7.org/fhir/us/davinci-alerts/GraphDefinition/admit-1"/>
	<name value="AlertAdmit"/>
	<status value="active"/>
	<date value="2019-08-21"/>
	<publisher value="Da Vinci Project"/>
	<contact>
		<telecom>
			<system value="url"/>
			<value value="http://www.hl7.org/about/davinci/"/>
		</telecom>
	</contact>
	<description value="Defines Da Vinci Alerts patient admission notication message bundle graph"/>
	<!--    this graph starts with a MessageHeader -->
	<start value="MessageHeader"/>
<!--
		<profile value="http://hl7.org/fhir/us/davinci-alerts/MessageHeader/admit-1"/>
-->
	<!--    define the MessageHeader links    -->
	<link>
		<path value="MessageHeader.focus"/>
		<min value="1"/>
		<max value="1"/>
		<target>
			<type value="Encounter"/>
			<profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-encounter"/>
			<!--    and inside this target any more links  -->
			<link>
				<path value="Encounter.subject"/>
				<min value="1"/>
				<max value="1"/>
				<target>
					<type value="Patient"/>
					<profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-patient"/>
					<link>
						<min value="0"/>
						<max value="1"/>
						<target>
							<type value="Coverage"/>
							<params value="target={beneficiary}"/>
							<profile value="http://hl7.org/fhir/us/davinci-hrex/StructureDefinition/hrex-coverage"/>
						</target>
					</link>
				</target>
			</link>
			<link>
				<path value="Encounter.diagnosis.condition"/>
				<min value="1"/>
				<max value="1"/>
				<target>
					<type value="Condition"/>
					<profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-condition"/>
				</target>
			</link>
			<link>
				<path value="Encounter.location"/>
				<min value="0"/>
				<max value="1"/>
				<target>
					<type value="Location"/>
					<profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-location"/>
				</target>
			</link>
		</target>
	</link>
	<link>
		<path value="MessageHeader.definition"/>
		<min value="0"/>
		<max value="1"/>
		<target>
			<type value="MessageDefinition"/>
<!--
			<profile value="http://hl7.org/fhir/us/davinci-alerts/MessageDefinition/alert"/>
-->
		</target>
	</link>
	<link>
		<path value="MessageHeader.sender"/>
		<min value="0"/>
		<max value="1"/>
		<target>
			<type value="Organization"/>
			<profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-organization"/>
		</target>
		<target>
			<type value="Practitioner"/>
			<profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-practitioner"/>
		</target>
		<target>
			<type value="PractitionerRole"/>
			<profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-practitionerrole"/>
		</target>
	</link>
	<link>
		<path value="MessageHeader.destination.receiver"/>
		<min value="0"/>
		<max value="1"/>
		<target>
			<type value="Organization"/>
			<profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-organization"/>
			<link>
				<path value="Organization.endpoint"/>
				<min value="0"/>
				<max value="1"/>
				<target>
					<type value="Endpoint"/>
					<profile value="http://hl7.org/fhir/us/davinci-alerts/StructureDefinition/alerts-endpoint"/>
				</target>
			</link>
		</target>
		<target>
			<type value="Practitioner"/>
			<profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-practitioner"/>
			<link>
				<path value="Organization.endpoint"/>
				<min value="0"/>
				<max value="1"/>
				<target>
					<type value="Endpoint"/>
					<profile value="http://hl7.org/fhir/us/davinci-alerts/StructureDefinition/alerts-endpoint"/>
				</target>
			</link>
		</target>
		<target>
			<type value="PractitionerRole"/>
			<profile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-practitionerrole"/>
			<link>
				<path value="Organization.endpoint"/>
				<min value="0"/>
				<max value="1"/>
				<target>
					<type value="Endpoint"/>
					<profile value="http://hl7.org/fhir/us/davinci-alerts/StructureDefinition/alerts-endpoint"/>
				</target>
			</link>
		</target>
	</link>
</GraphDefinition>
